{
  "name": "vue-live",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --entry ./src/index.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "./dist/vue-live.common.js",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "acorn": "^6.1.1",
    "buble": "^0.19.7",
    "core-js": "^2.6.5",
    "prismjs": "^1.16.0",
    "rewrite-imports": "^2.0.3",
    "vue": "^2.6.10",
    "vue-prism-editor": "^0.2.0",
    "vue-template-compiler": "^2.5.21",
    "walkes": "^0.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-unit-jest": "^3.6.3",
    "@vue/cli-service": "^3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "raw-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/*.unit.js"
        ],
        "parserOptions": {
          "parser": "babel-eslint",
          "sourceType": "module"
        },
        "env": {
          "jest": true
        },
        "globals": {
          "mount": false,
          "shallowMount": false,
          "createComponentMocks": false
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
